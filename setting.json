{
  "version": 1.0,
  "host": "0.0.0.0",
  "port": 8080,
  "connect":"redis://:123456@127.0.0.1:6379/0",
  "table_name": "default_pool",
  "proxy_src": [
      "freeProxy01",
      "freeProxy02",
      "freeProxy03",
      "freeProxy04",
      "freeProxy05",
      "freeProxy06",
      "freeProxy07",
      "freeProxy08",
      "freeProxy09"
  ],

  "http_test_url": "http://httpbin.org",
  "https_test_url":"https://httpbin.org",

  "verify_timeout": 10,
  "max_fail_count": 0,
  "pool_size_min": 20,
  "timezone": "Asia/Shanghai",

  "max_cached_proxy_size ": 100,

  "log_format": "%(asctime)s %(filename)s[line:%(lineno)d] %(levelname)s %(message)s",
  "logfile_format": "file %(asctime)s %(filename)s[line:%(lineno)d] %(levelname)s %(message)s",
  "log_level": "INFO",
  "log_to_file": true,

  "fetch_thread_num": 20,
  "check_thread_num": 20,
  "process_pool_max_worker": 5
}


