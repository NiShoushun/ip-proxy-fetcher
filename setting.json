{
  "version": 1.0,
  "host": "0.0.0.0",
  "port": 8080,
  "connect":"redis://:123456@127.0.0.1:6379/0",
  "table_name": "default_pool",
  "proxy_src": [
    "freeProxyFetcher01",
    "freeProxyFetcher02",
    "freeProxyFetcher03",
    "freeProxyFetcher04",
    "freeProxyFetcher05",
    "freeProxyFetcher06",
    "freeProxyFetcher07"
  ],

  "http_test_url": "http://httpbin.org",
  "https_test_url":"https://baidu.com",

  "verify_timeout": 10,
  "max_fail_count": 2,
  "pool_size_min": 20,

  "max_cached_proxy_size ": 10000,

  "log_format": "%(asctime)s %(filename)s[line:%(lineno)d] %(levelname)s %(message)s",
  "logfile_format": "%(asctime)s %(filename)s[line:%(lineno)d] %(levelname)s %(message)s",
  "log_level": "DEBUG",
  "log_to_file": true,

  "fetch_thread_num": 20,
  "check_thread_num": 20,
  "process_pool_max_worker": 5,
  "fetch_job_interval": 60,
  "check_job_interval": 90,

  "timezone": "Asia/Shanghai"
}


